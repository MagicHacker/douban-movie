.list-detail-wrap{
  width: 100%;
  height: 100%;
  background-color:rgb(243, 240, 240);
}
.list-detail-item{
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
  padding: 30rpx;
  margin-bottom: 20rpx;
}
.list-detail-rank{
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  text-align: center;
  background-color: bisque;
  border-radius: 10rpx;
}
.list-detail-content{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 30rpx;
}
.list-detail-photo{
  width: 200rpx;
  height: 250rpx;
  border-radius: 8rpx;
}
.list-detail-profile{
  width: 300rpx;
  height: 100%;
}
.list-detail-line{
  width: 1rpx;
  height:100%;
  border-right: 2rpx dotted #ccc;
}
.list-detail-like{
  width: 60rpx;
  height: 60rpx;
}
.list-detail-comment{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  padding-left: 30rpx;
  font-size: 32rpx;
  background-color: rgb(233, 231, 231);
  border-radius: 6rpx;
  margin-top: 30rpx;
}